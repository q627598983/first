class SelectionSort{
  public static void main(String[] args){
    int[] arr = {3,5,3,1,7,8,3,2};
    for(int i=0;i<arr.length-1;i++){
      int min = i;
      for(int j=1;j<arr.length;j++){
        if(arr[i]>arr[j]){
          min = j;
        }
      }
      int temp = arr[i];
      arr[i]=arr[min];
      arr[min]=temp;
    }
    for(int i=0;i<arr.length;i++){
    System.out.print(arr[i]);
    }
  }
}
